{
  "node": true,
  "browser": true,
  "esnext": true,
  "bitwise": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": false,
  "immed": true,
  "indent": 4,
  "latedef": "nofunc",
  "sub": true,
  "newcap": true,
  "noarg": true,
  "quotmark": "single",
  "regexp": true,
  "undef": true,
  "unused": false,
  "strict": true,
  "trailing": true,
  "smarttabs": true,
  "laxcomma": true,
  "expr": true,//disable: Expected an assignment or function call and instead saw an expression
  "-W116": false, // disable: Expected '===' and instead saw '=='.
  "-W041": false, //disable Use '===' to compare with '0'.
  "globals": {
    "toastr": true,
    "jQuery": true,
    "angular": true,
    "console": true,
    "$": true,
    "_": true,
    "Q": true,
    "moment": true,
    "module": true,
    "inject": true,
    "describe": true,
    "xdescribe": true,
    "it": true,
    "xit": true,
    "afterEach": true,
    "beforeEach": true,
    "dump": true,
    "assert": true,
    "expect": true,
    "browser": true,
    "element": true,
    "by": true,
    "sinon": true,
    "waitsForAndThenRun": true,
    "testModule": true,
    "vsprintf": true,
    "myUtil": true,
    "myMocks": true
  }
}
