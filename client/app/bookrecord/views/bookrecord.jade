form.enter-as-tab.on-focus(id="bookrecordForm", logic-form="bookrecord", name="bookrecordForm", novalidate, ng-submit="model.submitForm()", ng-model-options="{ debounce: 0 }")
    h1 Registo de Livro
        span.form-buttons
            button.btn.btn-default(tabindex="10000",ng-disabled="model.bookrecordLogicForm.$pristine()", type="button", ng-click="model.clearForm()") Limpar
            button.btn.btn-default(tabindex="9999",ng-show="bookrecordForm.reference.$viewValue", type="button", ng-click="model.newForm()") Novo
            button.btn.btn-default(tabindex="9998",ng-disabled="model.bookrecordLogicForm.$invalid()", type="submit", id="submit") Gravar


    .row.caixa
        .col-sm-12
            .row
                .col-xs-5.col-sm-3
                    label(for="reference") {{::model.labels.reference}}
                        input(value="{{::model.book.reference}}", disabled, id="reference", name="reference")
                    //{{::model.labels.reference}}
            .row
                .col-sm-12
                    label(for="title") {{::model.labels.title}}
                        input.font-major(type="text", tabindex="100", name="title", ng-model="model.book.title", autofocus, required, aria-label="{{::model.placeholders.title}}", placeholder="{{::model.placeholders.title}}")
                        div.tool-tip.bottom(ng-show="bookrecordForm.title.$dirty && bookrecordForm.title.$error.required") {{::model.valMessages.title.required}}

            .row
                .col-sm-12
                    label(for="subtitle") {{::model.labels.subtitle}}
                        input(type="text", tabindex="200", name="subtitle", ng-model="model.book.subtitle", aria-label="{{::model.placeholders.subtitle}}", placeholder="{{::model.placeholders.subtitle}}")
            .row
                .col-sm-6
                    label(for="authors") {{::model.labels.authors}}
                        span(angular-typeaheadjs,  angty-onselect="model.events.selectOption", angty-options="{{model.config.authors.options}}", angty-ttoptions="{{model.config.authors.ttoptions}}")
                            input.typeahead(type="text", tabindex="300" name="authors", ng-model="model.book.author", aria-label="{{::model.placeholders.authors}}", placeholder="{{::model.placeholders.authors}}")

                .col-sm-6(ng-show="model.book.authors.length>1")
                    label.label-items

                        span.item(ng-repeat="item in model.book.authors track by $index")
                            i.fa.fa-times(ng-click="model.removeItem()")
                            | {{item}}

    .row.caixa
        .col-sm-4
            label(for="editionPublisher") {{::model.labels.editionPublisher}}
